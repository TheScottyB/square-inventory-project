# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
IMAGE_SOURCE_DIR=./
IMAGE_OUTPUT_DIR=./output
LOG_LEVEL=info

# Agent Configuration
MAX_DESCRIPTION_LENGTH=500
GROUPING_SIMILARITY_THRESHOLD=0.8
ENABLE_DRY_RUN=false

# Square Dashboard Credentials (for PuppeteerSEOAgent)
# WARNING: Keep these credentials secure and never commit them to version control
SQUARE_EMAIL=your-square-email@example.com
SQUARE_PASSWORD=your-secure-password

# Environment Settings
NODE_ENV=development

# SEO Agent Settings
SEO_RETRIES=3
SEO_TIMEOUT=30000
SEO_CONCURRENCY=1
HEADLESS=false
