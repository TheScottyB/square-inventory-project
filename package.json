{
  "name": "square-inventory-project",
  "version": "1.0.0",
  "description": "AI-powered image processing workflow for Square inventory management using OpenAI Agent SDK",
  "main": "scripts/run-workflow.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/production/download-and-attach-from-json.js",
    "square:test": "node scripts/utilities/test-square-catalog-agent.js",
    "square:direct-attach": "node scripts/production/direct-attach-to-guitar.js",
    "square:download-and-attach": "node scripts/production/download-and-attach-from-json.js",
    "square:download-spocket": "node scripts/utilities/download-spocket-images.js",
    "ai:analyze-and-rename": "node scripts/production/analyze-and-rename-images.js",
    "ai:analyze-and-rename:dry-run": "node scripts/production/analyze-and-rename-images.js --dry-run",
    "ai:analyze-only": "node scripts/production/analyze-and-rename-images.js --no-rename",
    "ai:organize-products": "node scripts/production/organize-products-by-analysis.js",
    "ai:organize-products:dry-run": "node scripts/production/organize-products-by-analysis.js --dry-run",
    "ai:organize-products:clean": "node scripts/production/organize-products-by-analysis.js --clean",
    "ai:organize-by-source": "node scripts/production/organize-by-source.js",
    "ai:organize-by-source:dry-run": "node scripts/production/organize-by-source.js --dry-run",
    "ai:organize-by-source:clean": "node scripts/production/organize-by-source.js --clean",
    "generate-filenames": "node scripts/production/generate-filenames.js",
    "generate-filenames:apply": "node scripts/production/generate-filenames.js --apply --backup",
    "generate-filenames:dry-run": "ENABLE_DRY_RUN=true node scripts/production/generate-filenames.js",
    "download-and-organize-images": "node scripts/production/download-and-organize-images.js",
    "manage-inventory": "node scripts/production/manage-inventory.js",
    "manage-inventory:dry-run": "node scripts/production/manage-inventory.js assets/images --dry-run",
    "manage-inventory:auto-rename": "node scripts/production/manage-inventory.js assets/images --auto-rename",
    "experimental:version-test": "node scripts/experimental/version-testing/test-version-safe-upsert.js",
    "experimental:integration-test": "node scripts/experimental/version-testing/test-version-safe-integration.js",
    "experimental:core-features": "node scripts/experimental/version-testing/test-core-version-features.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:config": "jest src/config",
    "test:upload": "node tests/testRealImageUpload.js",
    "test:orchestrator": "jest test/orchestrator-dry-run.test.js",
    "lint": "eslint src/ scripts/",
    "lint:fix": "eslint src/ scripts/ --fix",
    "validate": "pnpm run lint && pnpm run test",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "seo:enhance-data": "node scripts/enhance-seo-data.js",
    "seo:export-snapshot": "node scripts/export-seo-snapshot.js",
    "seo:setup-chrome": "./setup-chrome-debugging.sh",
    "seo:launch-chrome": "./launch-chrome-for-seo.sh",
    "seo:launch-chrome-debug": "node scripts/launch-chrome-debug.js",
    "seo:test": "node testSEOAgent.js",
    "seo:run-agent": "node runPuppeteerAgent.js",
    "seo:run-agent:headless": "HEADLESS=true node runPuppeteerAgent.js",
    "intelligence:analyze": "node scripts/analyze-inventory-intelligence.js",
    "intelligence:analyze:fallback": "OPENAI_API_KEY=fallback node scripts/analyze-inventory-intelligence.js",
    "categories:apply": "node scripts/apply-categorization.js",
    "categories:apply:dry-run": "node scripts/apply-categorization.js --dry-run"
  },
  "keywords": [
    "openai",
    "ai",
    "image-processing",
    "inventory-management",
    "square",
    "agent-sdk"
  ],
  "author": "ScottyB",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "volta": {
    "node": "22.18.0"
  },
  "dependencies": {
    "@openai/agents": "^0.0.14",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.11.0",
    "ora": "^8.2.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "playwright": "^1.54.2",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.15.0",
    "square": "43.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5"
  }
}
