{
  "name": "square-inventory-project",
  "version": "1.0.0",
  "description": "AI-powered image processing workflow for Square inventory management using OpenAI Agent SDK",
  "main": "scripts/run-workflow.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/run-workflow.js",
    "manage-images": "node scripts/run-workflow.js",
    "manage-images:dry-run": "ENABLE_DRY_RUN=true node scripts/run-workflow.js",
    "generate-filenames": "node scripts/generate-filenames.js",
    "generate-filenames:apply": "node scripts/generate-filenames.js --apply --backup",
    "generate-filenames:dry-run": "ENABLE_DRY_RUN=true node scripts/generate-filenames.js",
    "square:test": "node scripts/test-square-catalog-agent.js",
    "square:upload-images": "node scripts/upload-images.js",
    "square:process-items": "node scripts/process-items.js",
    "square:process-directory": "node scripts/process-items.js -d",
    "square:process-file": "node scripts/process-items.js -f",
    "square:test-version-safe": "node scripts/test-version-safe-upsert.js",
    "square:test-integration": "node scripts/test-version-safe-integration.js",
    "square:test-core-features": "node scripts/test-core-version-features.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:config": "jest src/config",
    "lint": "eslint src/ scripts/",
    "lint:fix": "eslint src/ scripts/ --fix",
    "validate": "pnpm run lint && pnpm run test",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix"
  },
  "keywords": [
    "openai",
    "ai",
    "image-processing",
    "inventory-management",
    "square",
    "agent-sdk"
  ],
  "author": "ScottyB",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "volta": {
    "node": "22.18.0"
  },
  "dependencies": {
    "@openai/agents": "^0.0.14",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.0",
    "openai": "^5.11.0",
    "square": "43.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5"
  }
}
