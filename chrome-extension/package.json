{
  "name": "square-automation-extension",
  "version": "1.0.0",
  "description": "Chrome extension for Square Dashboard automation with OpenAI agents",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "package": "npm run build && web-ext build --source-dir=dist"
  },
  "dependencies": {
    "openai": "^4.24.1",
    "@openai/agents": "^0.1.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "web-ext": "^7.8.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "chrome-extension",
    "square",
    "automation",
    "openai",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}
